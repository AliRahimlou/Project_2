<div id="home">
  <div class="row">
    <div class="col s8 center-block">
      <h1 class="center">{{msg}}</h1>
    </div>
  </div>
</div>

<br>

<div class="row">
  <div class="col s12 m6">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">Litty Chat!</span>
        <div id="chat-window">
          <div id="output"></div>
          <div id="feedback"></div>
        </div>
      </div>
      <div class="card-action">
        <input id="handle" type="text" placeholder="Handle" />
        <input id="message" type="text" placeholder="Message" />
        <button class="waves-effect waves-light btn" id="sendIt">SEND IT</button>
      </div>
    </div>
  </div>
</div>